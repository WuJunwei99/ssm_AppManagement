<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.ncu.zte.mapper.ContractMapper">


  	<select id="selectAll" resultType="Contract">
		select * from contract
	</select>
	
	<select id="selectContractByStudentnum" resultType="Contract">
		select * from contract where student_num=#{studentNum}
	</select>
	
	<select id="selectContractById" resultType="Contract">
		select * from contract where id=#{id}
	</select>
	
  </mapper>