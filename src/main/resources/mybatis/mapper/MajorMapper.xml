<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.ncu.zte.mapper.MajorMapper">
  
  	<select id="selectByPrimaryKey" resultType="Major">
		select * from major where cityID=#{cityID}
	</select>
	
	 <select id="selectByParent" resultType="Major">
		select * from major where department_id = #{departmentId}
	</select>
	
  </mapper>